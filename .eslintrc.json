{
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-unused-expressions": 0,
      "no-mixed-operators": 0,
      "no-case-declarations": 0,
      "max-len": 0,
      "no-param-reassign": 0,
      "no-console": 0,
      "no-var": "error",
      "semi": "error",
      "indent": "error",
      "no-multi-spaces": "error",
      "space-in-parens": "error",
      "no-multiple-empty-lines": "error",
      "prefer-const": "error",
      "no-use-before-define": "error"
    }
}